<div>
  <a href="javascript://" class="field-hide" tabindex="-1">hide</a>
  <datepicker-component
      data-hide-title="{{configStartDate.label}}"
      label:from="configStartDate.label"
      date:bind="instance.start_date"
      setMaxDate:from="instance.end_date"
      helptext:from="configStartDate.helpText"
      readonly:from="readonly"
      disabled:from="disabled"
      {{#configStartDate.required}}required:from="true"{{/configStartDate.required}}
  />
</div>
