<div
  class="scroll-top-wrapper"
  on:el:scroll="onScroll"
>
  <content/>
  <button
    type="button"
    class="scroll-top-wrapper__button {{onTopButtonCssClass}}"
    on:el:click="scrollTop"
  >
    <i class="fa fa-arrow-up" aria-hidden="true"></i>
    Back to top
  </button>
</div>
